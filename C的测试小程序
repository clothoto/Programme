#include <stdio.h>

int main() {
    int num1, num2, sum;

    // 提示用户输入第一个整数
    printf("请输入第一个整数: ");
    scanf("%d", &num1);

    // 提示用户输入第二个整数
    printf("请输入第二个整数: ");
    scanf("%d", &num2);

    // 计算两个整数的和
    sum = num1 + num2;

    // 输出结果
    printf("它们的和是: %d\n", sum);

    return 0;
}
